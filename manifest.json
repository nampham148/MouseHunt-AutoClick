{
  "name": "MouseHunt Autoclick",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is an autoclick for horn hunting at mousehuntgame.com",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "MouseHunt",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "tabs",
    "https://www.mousehuntgame.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.mousehuntgame.com/*"
      ],
      "js": [
        "/js/jquery/jquery.js",
        "/src/page_action/page_action.js"        
      ]
    },
    {
      "matches": [
        "https://www.google.com/search?ei=uN4lXMqxJsKgsQXQ-5mwAg&q=googlce+click&oq=googlce+click&gs_l=psy-ab.3..0i13l4j0i13i10j0i13l5.4317.6129..6403...0.0..0.107.1045.12j1......0....1..gws-wiz.......0j0i71j35i39j0i67j0i20i263j0i10.kuQ1SiyNqQo"
      ],
      "js": [
        "/js/jquery/jquery.js",
        "/src/page_action/button_press.js"
      ]
    }
  ]
}